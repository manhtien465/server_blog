{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "myblog-install": "npm install  --prefix myblog",
    "start": "node ./bin/www",
    "www": "nodemon ./bin/www",
    "myblog": "npm start --prefix myblog",
    "dev": "concurrently\"npm run www\" \"npm run myblog\"",
    "heroku-postbuild":"NPM CONFIG_PRODUCTION=false npm install --prefix myblog && npm run build --prefix myblog"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "errorhandler": "^1.5.1",
    "express": "^4.16.4",
    "express-session": "^1.17.0",
    "http-errors": "^1.6.3",
    "jade": "^1.11.0",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "pg": "^7.18.1",
    "socket.io": "^2.3.0"
  }
}
